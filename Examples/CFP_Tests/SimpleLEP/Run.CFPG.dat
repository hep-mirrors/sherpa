(run){
  #
  # general settings
  EVENTS 50k; FRAGMENTATION Off;
  EVENT_SEED_MODE 3;
  #
  % Has to stay until soft photons are fixed
  YFS_MODE = 0
  #
  # collider setup
  MODEL HEFT; YUKAWA[11] 1.777;
  BEAM_1 11 45.6; BEAM_2 -11 45.6;
  PDF_SET None;
  #
  # shower setup
  #
  #
  SHOWER_GENERATOR CFP
  #
  SCALES VAR{Abs2(p[2]+p[3])};
  CSS_FS_AS_FAC 1.;
  #CSS_IS_AS_FAC 1.;
  CSS_FS_PT2MIN 1.;
  #CSS_IS_PT2MIN 1.;

  
  CSS_KIN_SCHEME            = 2    # 1 = CSS, 2 = ALARIC
  CSS_KFACTOR_SCHEME        = 1    # order of K-factor   
  CSS_ENDPOINT_SCHEME       = 0    # not yet fully done
  CSS_SOFTCORRECTION_SCHEME = 0    # not yet fully done
  CSS_COUPLING_SCHEME       = 1    # not yet fully done
  CSS_MUR_SCHEME            = 1    # 0 = t, 1 = kt2
  CSS_LOG_TYPE              = 3    # 1 = soft, 2 = coll
  CSS_SF_TYPE               = 1    # 1 = q->qg, 2 = g->gg, 4 = g->qq 
  CSS_MAXEM                 = 100

  RESPECT_MASSIVE_FLAG 1;
  CSS_TC_ENHANCE 5;           # this is for the 1->3 splittings
  PDF_LIBRARY LHAPDFSherpa; USE_PDF_ALPHAS 7;
  #
  # analysis
  ANALYSIS Rivet
  ANALYSIS_OUTPUT Alaric_qq_NoG_kt2
}(run)

(processes){
  Process 11 -11 -> 1 -1
  Order (*,2,0);
  End process;
}(processes)

(analysis){
  BEGIN_RIVET {
  -a LL_JetRates
  } END_RIVET
}(analysis)